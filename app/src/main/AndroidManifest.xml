<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mikhalychgarage">

    <uses-permission android:name="android.permission.CAMERA" />

    <uses-feature android:name="android.hardware.camera" />


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.mikhalychgarage.ScanCodeActivitysearch"></activity>
        <activity android:name="com.example.mikhalychgarage.ScanCodeActivity" />
        <activity android:name="com.example.mikhalychgarage.ScanCodeActivitydel" />
        <activity android:name="com.example.mikhalychgarage.viewInventoryActivity" />
        <activity android:name="com.example.mikhalychgarage.scanItemsActivity" />
        <activity android:name="com.example.mikhalychgarage.deleteItemsActivity" />
        <activity android:name="com.example.mikhalychgarage.additemActivity" />
        <activity android:name="com.example.mikhalychgarage.dashboardActivity" />
        <activity android:name="com.example.mikhalychgarage.RegisterActivity" />
        <activity android:name="com.example.mikhalychgarage.LoginActivity" />
        <activity android:name="com.example.mikhalychgarage.MainActivity" />
        <activity
            android:name="com.example.mikhalychgarage.SplashsActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>

</manifest>